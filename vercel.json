{
  "rewrites": [
    { 
      "source": "/", 
      "destination": "/htmlPage/index.html" 
    },
    { 
      "source": "/auth/(.*)", 
      "destination": "/auth/$1" 
    },
    { 
      "source": "/AdminPage/(.*)", 
      "destination": "/AdminPage/$1" 
    },
    { 
      "source": "/htmlPage/(.*)", 
      "destination": "/htmlPage/$1" 
    }
  ],
  "redirects": [
    {
      "source": "/login",
      "destination": "/auth/login.html",
      "permanent": false
    },
    {
      "source": "/admin",
      "destination": "/AdminPage/index.html", 
      "permanent": false
    }
  ]
}